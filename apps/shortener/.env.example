APP_NAME=PocketURL
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost:8000

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=mysql
DB_PORT=3306
DB_DATABASE=pocket_url
DB_USERNAME=shortener
DB_PASSWORD=secret

SESSION_DRIVER=redis
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=redis

CACHE_STORE=redis
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=redis
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

SHORT_DOMAIN=
SHORTCODE_LENGTH=6
SHORTCODE_POOL_SIZE=10000
SHORTCODE_POOL_MIN_SIZE=1000
SHORTCODE_REFILL_BATCH_SIZE=1000

# ==================================
# CORS Configuration
# ==================================
CORS_ALLOWED_ORIGINS=
# Throw exception on invalid CORS config (recommended for CI/CD)
STRICT_CORS_VALIDATION=false

# ==================================
# Octane Configuration
# ==================================
OCTANE_SERVER=roadrunner
OCTANE_WORKERS=1
OCTANE_MAX_REQUESTS=500
OCTANE_ROADRUNNER_BINARY=/usr/local/bin/rr

# ==================================
# RabbitMQ Configuration
# ==================================
RABBITMQ_HOST=rabbitmq
RABBITMQ_PORT=5672
RABBITMQ_USER=admin
RABBITMQ_PASSWORD=admin
RABBITMQ_VHOST=/
RABBITMQ_QUEUE=cache.invalidation

# ==================================
# Analytics configuration
# ==================================
ANALYTICS_API_KEY=
ANALYTICS_API_ENDPOINT=
